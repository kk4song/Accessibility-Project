{
    "manifest_version": 3,
    "name": "Accessibility Project",
    "version": "1.0",
    "description": "Apply a magnifier effect on the browser.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png"
      }
    }
  }
  